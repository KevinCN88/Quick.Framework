<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <!-- 模块名[.子模块名1][.XXX].功能描述，Qf开头的为框架内置资源-->

    <!-- 覆盖系统字符串，自定义MessageBox标题 -->
    <sys:String x:Key="Qf.MsgBox.DefaultWndTitle">Quick框架演示程序</sys:String>

    <!-- 应用程序字符串 -->
    <sys:String x:Key="App.MainWindow.Title">QuickFramework - 转换器演示</sys:String>
    <sys:String x:Key="App.ExitWarning">确定要退出吗？</sys:String>
    
    <sys:String x:Key="App.Mainwindow.GetLanguageButtonText">获取当前语言</sys:String>
    <sys:String x:Key="App.Mainwindow.GetMyOccupationButtonText">获取我的职业</sys:String>
    <sys:String x:Key="App.MainWindow.OccupationTitle">职业：</sys:String>
    <sys:String x:Key="App.Mainwindow.AppComment">这是一个Quick框架的多语言演示程序</sys:String>

    <sys:String x:Key="Demo.Edit.Occupation.NoSelect">未选择</sys:String>
    <sys:String x:Key="Demo.Edit.Occupation.Student">学生</sys:String>
    <sys:String x:Key="Demo.Edit.Occupation.Teacher">老师</sys:String>

    <sys:String x:Key="Demo.Edit.NotSelectOccupationMsgTip">你没有选择任何职业！</sys:String>
    <sys:String x:Key="Demo.Edit.HasSelectedOccupationMsgTip">你的职业是：{0}</sys:String>

    <sys:String x:Key="App.CurrentLanguageChangedLog">当前语言切换为{0}</sys:String>
</ResourceDictionary>